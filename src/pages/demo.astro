---
// Components
// - Layout
import Layout from '../layouts/Layout.astro'
// - UI
import DemoHero from '../components/blocks/hero/DemoHero.astro'
import CTA from '../components/blocks/CTA/BasicDark.astro'
import ContactCards from '../components/blocks/contact/ContactCards.astro'
import SocialProof from '../components/blocks/socialproof/Basic.astro'

// Content
// - SEO
const SEO = {
	title: 'Multi Framework | Idea to App Store in Under an Hour',
	description:
		'Multi is the fastest way to turn your app idea into a real, deployable application. Build, test, and launch in under 60 minutes with our all-in-one platform.',
}
// - Page Header
const header = {
	title: '<strong>Multi Framework</strong> In Action.',
	text: "Multi Framework includes a demo app to help you jump start development. Find it on the App Store, Play Store and the Web."
}
---

<Layout title={SEO.title} description={SEO.description}>
	<DemoHero
		title={header.title}
		text={header.text}
		id="contact"
		classes="bg-neutral-50 dark:bg-neutral-950/80"
	/>
	<!-- <ContactCards classes="dark:bg-neutral-950/80" /> -->
	<CTA />
</Layout>
